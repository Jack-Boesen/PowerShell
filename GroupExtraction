Install-Module AzureAD
Install-Module MicrosoftTeams

Connect-AzureAD
Connect-MicrosoftTeams

$groups = Get-AzureADGroup
$groups | Select-Object DisplayName, Description, Mail, GroupTypes

$teams = Get-Team
$teams | Select-Object DisplayName, MailNickname | Export-Csv -Path "C:\Users\JackBoesen\OneDrive - Kru-Marc Inc\Kru-Marc Inc (Individual)"

Write-Output "Group lists have been exported successfully."
